{
    "manifest_version": 2,
    "name": "GrapHistory",
    "version": "0.1",
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["src/main.js"]
        }
    ],
    "browser_action":{
        "default_title": "GrapHistory",
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "48.png",
        "128": "128.png",
        "16": "16.png"
    }
}